    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fs-extra": "^9.0.1",
    "husky": "^4.2.5",
    "istanbul-instrumenter-loader": "^3.0.1",
    "jest": "^26.4.2",
    "jest-fetch-mock": "^3.0.3",
    "koa": "^2.13.1",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "koa-views": "^7.0.1",
